{
  "name": "restruct/silverstripe-cookiebar",
  "description": "Drop-in configurable notice about cookies",
  "type": "silverstripe-vendormodule",
  "license": "MIT",
  "keywords": [
    "silverstripe",
    "cookies"
  ],
  "authors": [ { "name": "Restruct", "email": "dev@restruct.nl", "homepage": "https://restruct.nl" } ],
  "require": {
    "silverstripe/framework": "^4 || ^5"
  },
  "extra": {
    "expose": [
      "client/dist/css",
      "client/dist/js"
    ],
    "installer-name": "cookie-bar"
  },
  "autoload": {
    "psr-4": {
      "Restruct\\CookieBar\\": "src/"
    }
  }
}
